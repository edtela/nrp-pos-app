{
  "name": "nrp-pos",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run generate-static",
    "build:client": "tsc && vite build",
    "generate-static": "tsx --loader ./build/css-loader.mjs build/generate-static.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^3.2.4"
  }
}
